{
  "name": "mezcalier-back",
  "version": "0.0.1",
  "description": "MezcalierBack",
  "author": {
    "email": "email@codetecs.mx",
    "name": "Xquenda Soluciones Tecnologicas S.A. de C.V.",
    "url": "https://codetecs.mx"
  },
  "private": true,
  "license": "COPYRIGHT 2020",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && npm run copy:assets",
    "format": "node_modules/.bin/prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run copy:assets && nest start",
    "start:dev": "npm run copy:assets && nest start --watch",
    "start:debug": "npm run copy:assets && nest start --debug --watch",
    "start:debug:dc": "npm run copy:assets && nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "npm run copy:assets && node dist/main",
    "lint": "node_modules/.bin/eslint \"{src,apps,libs,test}/**/*.ts\" --max-warnings 0",
    "lint:fix": "node_modules/.bin/eslint \"{src,apps,libs,test}/**/*.ts\" --max-warnings 0 --fix",
    "test:debug": "node --inspect -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config ./jest-e2e.json --detectOpenHandles --forceExit",
    "test": "node_modules/.bin/jest --config ./jest-e2e.json --detectOpenHandles --forceExit --runInBand",
    "test:cov": "node_modules/.bin/jest --config ./jest-e2e.json --detectOpenHandles --forceExit --coverage",
    "typeorm": "node --require ts-node/register -r ./tsconfig-paths-bootstrap.ts ./node_modules/typeorm/cli.js",
    "mig:gen": "node_modules/.bin/ts-node -r ./tsconfig-paths-bootstrap.ts ./node_modules/typeorm/cli.js migration:generate --config orm.config.json --name",
    "mig:run": "node_modules/.bin/ts-node -r ./tsconfig-paths-bootstrap.ts ./node_modules/typeorm/cli.js migration:run --config orm.config.json",
    "mig:rev": "node_modules/.bin/ts-node -r ./tsconfig-paths-bootstrap.ts ./node_modules/typeorm/cli.js migration:revert --config orm.config.json",
    "doc": "./node_modules/.bin/compodoc -p tsconfig.build.json -s -w --theme material --hideGenerator -o --customLogo src/assets/logo_md.png --customFavicon src/assets/logo_sintexto.png -n \"Documentación Mezcalier\"",
    "doc:gen": "./node_modules/.bin/compodoc -p tsconfig.build.json --theme material --hideGenerator --customLogo src/assets/logo_md.png --customFavicon src/assets/logo_sintexto.png -n \"Documentación Mezcalier\"",
    "doc:cov": "./node_modules/.bin/compodoc -p tsconfig.build.json --coverageMinimumPerFile 50 --coverageTest 80",
    "copy:assets": "echo 'Preparing templates...' && mkdir -p dist & cp -rp 'src/templates' 'dist' && cp -r 'src/assets' 'dist'",
    "precommit": "echo 'Running all tests...' && npm run truncate && npm run lint && npm run doc:cov && npm run build && npm run test:cov && echo 'Congratulations your commit will pass the pipeline!'&&exit 0",
    "drop": "bash drop-all-tables.sh",
    "truncate": "bash truncate-tables.sh",
    "X": "npm run drop && npm run mig:run && npm run seed && npm run test",
    "XX": "npm run truncate && npm run seed && npm run test",
    "gource:render": "cd .gource && bash render.sh",
    "gource:loop": "cd .gource && bash loop.sh",
    "seed": "node_modules/.bin/ts-node -r ./tsconfig-paths-bootstrap.ts src/seeder.ts",
    "seed:refresh": "node_modules/.bin/ts-node -r ./tsconfig-paths-bootstrap.ts src/seeder --refresh"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.8.3",
    "@google-cloud/vision": "^2.3.1",
    "@hapi/joi": "^17.1.1",
    "@nest-middlewares/response-time": "^6.0.0",
    "@nestjs-modules/mailer": "^1.5.1",
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.15",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/swagger": "^4.8.0",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjs/websockets": "^7.6.15",
    "@sentry/hub": "^6.2.5",
    "@sentry/minimal": "^6.2.5",
    "@sentry/node": "^6.2.5",
    "@types/sharp": "^0.28.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "nestjs-rate-limiter": "^2.7.0",
    "passport": "^0.4.1",
    "passport-facebook-token-nest": "^3.3.2",
    "passport-google-oauth20": "^2.0.0",
    "passport-google-verify-token": "^1.2.6",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pug": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.7",
    "sharp": "^0.28.1",
    "swagger-ui-express": "^4.1.6",
    "twilio": "^3.60.0",
    "typeorm": "^0.2.32"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.15",
    "@sentry/types": "^6.2.5",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.5.1",
    "@types/hapi__joi": "^17.1.6",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/passport-facebook": "^2.1.10",
    "@types/passport-facebook-token": "^0.4.34",
    "@types/passport-google-oauth20": "^2.0.7",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/qs": "^6.9.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "nestjs-seeder": "^0.2.0",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.1.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@mezcal/(.*)$": "<rootDir>/src/$1"
    }
  }
}
